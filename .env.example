# ssrok Server Configuration
# Rename this file to .env locally

# TLS Configuration
# Set to 'true' to enable TLS (valid certs must be present in certs/)
SSROK_ENABLE_TLS=false

# Port to listen on (default: 80, standard web port - privileged on macOS/Linux)
PORT=80

# Server URL for Client (Development)
SSROK_SERVER=http://localhost

# Log Level (debug, info, error)
LOG_LEVEL=info

# Rate Limiting
SSROK_RATE_LIMIT=60

# Redis Configuration (optional)
# If REDIS_HOST is set, sessions will be stored in Redis instead of memory
REDIS_HOST=
REDIS_PORT=6379
REDIS_USERNAME=
REDIS_PASSWORD=
